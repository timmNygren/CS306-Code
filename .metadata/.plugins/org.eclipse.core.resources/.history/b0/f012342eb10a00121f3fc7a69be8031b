package test;

import static org.junit.Assert.*;
import org.junit.*;

import money.Envelope;

public class TestEnvelope {

	@Test
	public void testGetTotalCents() {
		
		Envelope testEnvelope = new Envelope(1, 4, 10, 20, 100);
		long actual = testEnvelope.getTotalCents();
		long expected = 500;
		Assert.assertEquals(expected, actual);
		
		//fail("Not yet implemented");
	}
	
	@Test
	public void testGetTotalCents1() {
		
		Envelope testEnvelope = new Envelope(0, 3, 5, 7, 9);
		long actual = testEnvelope.getTotalCents();
		long expected = 169;
		Assert.assertEquals(expected, actual);
		
		//fail("Not yet implemented");
	}
	
}
