import java.io.*;
import java.util.*;


import javax.swing.JFileChooser;


public class ScoreTrakker {
	
	private ArrayList<Student> students;
	FileReader reader = null;
	
	
	public ScoreTrakker() {
		super();
		students = new ArrayList<Student>();
	}
	
	public void loadFromFile(String fileName) throws FullNameException{
		try {
		JFileChooser chooser = new JFileChooser();
			if (chooser.showOpenDialog(null) == JFileChooser.APPROVE_OPTION) {
				File selectedFile = chooser.getSelectedFile();
				reader = new FileReader(selectedFile);
			}
		}
		catch (FileNotFoundException exception) {
			
		}
	}
	
	public void printInOrder() {
		Collections.sort(students);
		for (int i = 0; i < students.size(); ++i) {
			System.out.println(students.get(i));
		}
	}


	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		ScoreTrakker scoreTrakker = new ScoreTrakker();
		scoreTrakker.loadFromFile("scores.txt");
		//scoreTrakker.loadFromFile("badscore.txt");
		//scoreTrakker.loadFromFile("badname.txt");
		//scoreTrakker.loadFromFile("something.txt");
		scoreTrakker.printInOrder();
	}

}
